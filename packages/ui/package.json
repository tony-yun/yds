{
  "name": "@tony-yun/ui",
  "version": "0.0.3",
  "description": "A React UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/button.d.ts",
      "import": "./dist/button.mjs",
      "require": "./dist/button.js"
    },
    "./card": {
      "types": "./dist/card.d.ts",
      "import": "./dist/card.mjs",
      "require": "./dist/card.js"
    },
    "./code": {
      "types": "./dist/code.d.ts",
      "import": "./dist/code.mjs",
      "require": "./dist/code.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "publish:npm": "yarn build && npm publish --access public --registry https://registry.npmjs.com",
    "publish:github": "yarn build && npm publish --registry https://npm.pkg.github.com",
    "publish:all": "yarn publish:npm && yarn publish:github"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system"
  ],
  "author": "Tony Yun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tony-yun/yds"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.34.0",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
